{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["../oskari-frontend/src/BasicBundle.js", "../oskari-frontend/src/README.md"],
        "exclude": ["../oskari-frontend/src/react"],
        "includePattern": ".+\\.js(x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": false,
        "destination": "generated/jsdoc/core"
    }
}
